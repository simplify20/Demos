<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yjj.rxdemo">

    <application
        android:name=".RxApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"></activity>
        <activity
            android:name=".lifecycle.RxLifecycleActivity"
            android:label="LifeCycle">

        </activity>
        <activity
            android:name=".rxbind.RxBindActivity"
            android:label="RxBinding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".rxbind.RxSwipeRefreshLayoutTestActivity"
            android:label="SwipeRefresh"></activity>
        <activity android:name=".rxbind.mvp.view.RefreshActivity">
        </activity>
        <activity android:name=".rxbind.mvp.view.LoginActivity"></activity>
    </application>

</manifest>
